int WolframLibrary_initialize( WolframLibraryData libData) {\n\treturn 0;\n}\n\
\nDLLEXPORT void WolframLibrary_uninitialize( WolframLibraryData libData) {\n\
\treturn;\n}\n\nDLLEXPORT int coins(WolframLibraryData libData, mint Argc, \
MArgument *Args, MArgument Res){\n\tMTensor uc=MArgument_getMTensor(Args[0]);\
\n\tmint* uc_data=libData->MTensor_getIntegerData(uc);\n\tmint \
rows=MArgument_getInteger(Args[1]);\n\tmint \
cols=MArgument_getInteger(Args[2]);\n\tmint dims[2]={rows,cols};\n\tmint \
err=0;\n\t\n\tmint count=0;\n\tMTensor status;\n\tmint* status_data;\n\terr = \
libData->MTensor_new(MType_Integer, 2, dims, &status);\n\tstatus_data = \
libData->MTensor_getIntegerData(status);\n\t\n\tfor (mint i=0;i<rows;i++) {\n\
\t\tfor (mint j=0;j<cols;j++) {\n\t\t\tif (i==0 || j==0) \
status_data[j+cols*i]=1;\n\t\t\telse status_data[j+cols*i]=0;\n\t\t}\n\t}\n\n\
\tfor (mint i=1; i<rows; i++){\n\t\tfor (mint j=1; j<cols; j++){\n\t\t\tif \
(j-uc_data[i]>=0) \
status_data[j+cols*i]=status_data[j+cols*(i-1)]+status_data[(j-uc_data[i])+\
cols*i];\n\t\t\telse status_data[j+cols*i]=status_data[j+cols*(i-1)];\n\t\t}\n\
\t}\n\t\n\tcount=status_data[(cols-1)+cols*(rows-1)];\n\t\
