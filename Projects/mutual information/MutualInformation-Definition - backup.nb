Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Condition", "[", 
    RowBox[{
     RowBox[{"MutualInformationError", "[", 
      RowBox[{"miEstimator_", ",", "x_List", ",", "y_List", ",", 
       RowBox[{"opts", ":", 
        RowBox[{"OptionsPattern", "[", "MutualInformationError", "]"}]}]}], 
      "]"}], ",", 
     RowBox[{
      RowBox[{"Length", "[", "x", "]"}], "==", 
      RowBox[{"Length", "[", "y", "]"}]}]}], "]"}], ":=", "\n", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"NT", "=", 
        RowBox[{"Length", "[", "x", "]"}]}], ",", 
       RowBox[{"n", "=", 
        RowBox[{"Range", "[", 
         RowBox[{"2", ",", "10"}], "]"}]}], ",", 
       RowBox[{"optf", "=", 
        RowBox[{"FilterRules", "[", 
         RowBox[{
          RowBox[{"{", "opts", "}"}], ",", 
          RowBox[{"Options", "[", "miEstimator", "]"}]}], "]"}]}]}], "}"}], 
     ",", "\n", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"xyn", "=", 
          RowBox[{"Transpose", "[", 
           RowBox[{"{", 
            RowBox[{"x", ",", "y"}], "}"}], "]"}]}], ",", "minfo", ",", 
         "varni"}], "}"}], ",", "\n", 
       RowBox[{
        RowBox[{"minfo", "=", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"miEstimator", "[", 
             RowBox[{
              RowBox[{"#", "[", 
               RowBox[{"[", 
                RowBox[{"All", ",", "1"}], "]"}], "]"}], ",", 
              RowBox[{"#", "[", 
               RowBox[{"[", 
                RowBox[{"All", ",", "2"}], "]"}], "]"}], ",", "optf"}], "]"}],
             "&"}], ",", 
           RowBox[{
            RowBox[{
             RowBox[{"Take", "[", 
              RowBox[{
               RowBox[{"Partition", "[", 
                RowBox[{"xyn", ",", 
                 RowBox[{"UpTo", "[", 
                  RowBox[{"Round", "[", 
                   RowBox[{"NT", "/", "#"}], "]"}], "]"}]}], "]"}], ",", 
               "#"}], "]"}], "&"}], "/@", "n"}], ",", 
           RowBox[{"{", "2", "}"}]}], "]"}]}], ";", "\n", 
        RowBox[{"varni", "=", 
         RowBox[{"Variance", "/@", "minfo"}]}], ";", "\n", 
        RowBox[{"Sqrt", "[", 
         RowBox[{
          RowBox[{"Total", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"n", "-", "1"}], ")"}], "/", "n"}], ")"}], "*", 
            "varni"}], "]"}], "/", 
          RowBox[{"Total", "[", 
           RowBox[{"(", 
            RowBox[{"n", "-", "1"}], ")"}], "]"}]}], "]"}]}]}], "\n", "]"}]}],
     "]"}]}], ";"}]], "Input",
