Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"MutualInformation", "[", 
    RowBox[{
     RowBox[{"distz_", "?", "DistributionParameterQ"}], ",", " ", 
     RowBox[{"OptionsPattern", "[", 
      RowBox[{"{", 
       RowBox[{"MutualInformation", ","}], "}"}], "]"}]}], "]"}], ":=", "\n", 
   
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"distx", "=", 
        RowBox[{"MarginalDistribution", "[", 
         RowBox[{"distz", ",", "1"}], "]"}]}], ",", 
       RowBox[{"disty", "=", 
        RowBox[{"MarginalDistribution", "[", 
         RowBox[{"distz", ",", "2"}], "]"}]}]}], "}"}], ",", "\n", 
     RowBox[{"NIntegrate", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"PDF", "[", 
         RowBox[{"distz", ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "y"}], "}"}]}], "]"}], "*", 
        RowBox[{"Log", "[", 
         RowBox[{"Simplify", "@", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"PDF", "[", 
             RowBox[{"distz", ",", 
              RowBox[{"{", 
               RowBox[{"x", ",", "y"}], "}"}]}], "]"}], "/", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"PDF", "[", 
               RowBox[{"distx", ",", "x"}], "]"}], "*", 
              RowBox[{"PDF", "[", 
               RowBox[{"disty", ",", "y"}], "]"}]}], ")"}]}], ")"}]}], 
         "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", 
         RowBox[{"-", "Infinity"}], ",", "Infinity"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"y", ",", 
         RowBox[{"-", "Infinity"}], ",", "Infinity"}], "}"}], ",", "\n", 
       RowBox[{"AccuracyGoal", "->", 
        RowBox[{"OptionValue", "[", 
         RowBox[{"MutualInformation", ",", " ", "AccuracyGoal"}], "]"}]}]}], 
      "]"}]}], "\n", "]"}]}], ";"}]], "Input",
