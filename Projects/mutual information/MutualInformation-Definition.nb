            RowBox[{"z", "[", "i", "]"}], " ", "from", " ", 
            RowBox[{"it", "'"}], "s", " ", "k"}], "-", 
           RowBox[{"th", " ", "neighbour", " ", "in", " ", "Z"}]}], ",", " ", 
          
          RowBox[{
           RowBox[{"picks", " ", "only", " ", "the", " ", "k"}], "-", 
