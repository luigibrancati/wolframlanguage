Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"MutualInformation", "[", 
    RowBox[{
     RowBox[{"distz_", "?", "DistributionParameterQ"}], ",", " ", 
     RowBox[{"opts", ":", 
      RowBox[{"OptionsPattern", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Options", "[", 
          RowBox[{
          "ResourceFunction", "[", "\"\<KullbackLeiblerDivergence\>\"", "]"}],
           "]"}], ",", "MutualInformation"}], "}"}], "]"}]}]}], "]"}], ":=", 
   "\n", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"domainz", "=", 
        RowBox[{"DistributionDomain", "[", "distz", "]"}]}], ",", " ", 
       RowBox[{"dim", "=", 
        RowBox[{"Length", "[", 
         RowBox[{"DistributionDomain", "[", "distz", "]"}], "]"}]}]}], "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"productDist", ",", "distx", ",", "disty", ",", " ", 
         RowBox[{"splt", "=", 
          RowBox[{"ReplaceAll", "[", 
           RowBox[{
            RowBox[{"OptionValue", "[", 
             RowBox[{"MutualInformation", ",", "\"\<Marginals\>\""}], "]"}], 
            ",", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{"l", ":", 
                RowBox[{"{", "___", "}"}]}], "\[RuleDelayed]", "l"}], ",", 
              RowBox[{"i_Integer", "\[RuleDelayed]", 
               RowBox[{"{", "i", "}"}]}]}], "}"}]}], "]"}]}]}], "}"}], ",", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"dim", "\[LessEqual]", "1"}], ",", 
          RowBox[{
           RowBox[{"Message", "[", 
            RowBox[{
             RowBox[{"MutualInformation", "::", "jointdist"}], ",", 
             "domainz"}], "]"}], ";", 
           RowBox[{"Return", "[", "$Failed", "]"}]}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Switch", "[", 
         RowBox[{"splt", ",", "Automatic", ",", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"distx", ",", "disty"}], "}"}], "=", 
           RowBox[{"With", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"splitHalf", "=", 
               RowBox[{"Round", "[", 
                RowBox[{"dim", "/", "2"}], "]"}]}], "}"}], ",", 
             RowBox[{"Map", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"MarginalDistribution", "[", 
                 RowBox[{"distz", ",", "#"}], "]"}], "&"}], ",", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"Range", "[", "splitHalf", "]"}], ",", 
                 RowBox[{"Range", "[", 
                  RowBox[{
                   RowBox[{"splitHalf", "+", "1"}], ",", "dim"}], "]"}]}], 
                "}"}]}], "]"}]}], "]"}]}], ",", 
          RowBox[{
           RowBox[{"{", "__Integer", "}"}], "/;", 
           RowBox[{"(", 
            RowBox[{"SubsetQ", "[", 
             RowBox[{
              RowBox[{"Range", "[", "dim", "]"}], ",", "splt"}], "]"}], 
            ")"}]}], ",", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"distx", ",", "disty"}], "}"}], "=", 
           RowBox[{"Map", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"MarginalDistribution", "[", 
               RowBox[{"distz", ",", "#"}], "]"}], "&"}], ",", 
             RowBox[{"{", 
              RowBox[{"splt", ",", 
               RowBox[{"Complement", "[", 
                RowBox[{
                 RowBox[{"Range", "[", "dim", "]"}], ",", "splt"}], "]"}]}], 
              "}"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{"{", "__Integer", "}"}], ",", 
          RowBox[{
           RowBox[{"Message", "[", 
            RowBox[{
             RowBox[{"MutualInformation", "::", "splitlist"}], ",", "splt", 
             ",", 
             RowBox[{"Range", "[", "dim", "]"}]}], "]"}], ";", 
           RowBox[{"Return", "[", "$Failed", "]"}]}], ",", 
          "\[IndentingNewLine]", "_", ",", 
          RowBox[{
           RowBox[{"Message", "[", 
            RowBox[{
             RowBox[{"MutualInformation", "::", "marginals"}], ",", "splt", 
             ",", "\"\<Marginals\>\""}], "]"}], ";", 
           RowBox[{"Return", "[", "$Failed", "]"}]}]}], "]"}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "this", " ", "bit", " ", "of", " ", "code", " ", "is", " ", "needed",
           " ", "to", " ", "fix", " ", "the", " ", "behaviour", " ", "of", 
          " ", "LogLikelihood", " ", "for", " ", "the", " ", 
          "ProductDistribution", " ", "of", " ", "multivariate", " ", 
          "distributions"}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"productDist", "/:", 
         RowBox[{
          RowBox[{"LogLikelihood", "[", 
           RowBox[{
            RowBox[{"productDist", "[", 
             RowBox[{"p_", ",", "q_"}], "]"}], ",", 
            RowBox[{"{", "vars_List", "}"}]}], "]"}], "/;", 
          RowBox[{"(", 
           RowBox[{"AllTrue", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"DistributionDomain", "[", "p", "]"}], ",", 
               RowBox[{"DistributionDomain", "[", "q", "]"}]}], "}"}], ",", 
             "ArrayQ"}], "]"}], ")"}]}], ":=", 
         RowBox[{"Simplify", "[", 
          RowBox[{
           RowBox[{"LogLikelihood", "[", 
            RowBox[{"p", ",", 
             RowBox[{"{", 
              RowBox[{"Take", "[", 
               RowBox[{"vars", ",", 
                RowBox[{"Length", "[", 
                 RowBox[{"DistributionDomain", "[", "p", "]"}], "]"}]}], 
               "]"}], "}"}]}], "]"}], "+", 
           RowBox[{"LogLikelihood", "[", 
            RowBox[{"q", ",", 
             RowBox[{"{", 
              RowBox[{"Drop", "[", 
               RowBox[{"vars", ",", 
                RowBox[{"Length", "[", 
                 RowBox[{"DistributionDomain", "[", "p", "]"}], "]"}]}], 
               "]"}], "}"}]}], "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"productDist", "/:", 
         RowBox[{"LogLikelihood", "[", 
          RowBox[{
           RowBox[{"productDist", "[", 
            RowBox[{"p_", ",", "q_"}], "]"}], ",", 
           RowBox[{"{", "vars_", "}"}]}], "]"}], ":=", 
         RowBox[{"Simplify", "[", 
          RowBox[{"LogLikelihood", "[", 
           RowBox[{
            RowBox[{"ProductDistribution", "[", 
             RowBox[{"p", ",", "q"}], "]"}], ",", 
            RowBox[{"{", "vars", "}"}]}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"productDist", "/:", 
         RowBox[{"f_", "[", 
          RowBox[{"productDist", "[", 
           RowBox[{"p_", ",", "q_"}], "]"}], "]"}], ":=", 
         RowBox[{"f", "[", 
          RowBox[{"ProductDistribution", "[", 
           RowBox[{"p", ",", "q"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
         "ResourceFunction", "[", "\"\<KullbackLeiblerDivergence\>\"", "]"}], 
         "[", 
         RowBox[{"distz", ",", 
          RowBox[{"productDist", "[", 
           RowBox[{"distx", ",", "disty"}], "]"}], ",", " ", 
          RowBox[{"FilterRules", "[", 
           RowBox[{
            RowBox[{"{", "opts", "}"}], ",", 
            RowBox[{"Options", "[", 
             RowBox[{
             "ResourceFunction", "[", "\"\<KullbackLeiblerDivergence\>\"", 
              "]"}], "]"}]}], "]"}]}], "]"}]}]}], "\[IndentingNewLine]", 
      "]"}]}], "]"}]}], ";"}]], "Input",
